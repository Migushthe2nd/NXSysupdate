{
  "name": "nxsysupdate",
  "version": "2.0.0",
  "description": "Posts to a Discord webhook when a new Nintendo Switch system update has been detected",
  "author": "Migush",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "nodemon --exec ts-node ./src/index.ts",
    "build": "tsc"
  },
  "dependencies": {
    "adm-zip": "^0.5.5",
    "axios": "^0.21.1",
    "axios-cookiejar-support": "^1.0.0",
    "cheerio": "1.0.0-rc.10",
    "config": "^3.3.9",
    "cookiejar": "^2.1.2",
    "discord.js": "14",
    "edit-json-file": "^1.4.0",
    "md5-file": "^5.0.0",
    "path": "^0.12.7",
    "tmp": "^0.2.1",
    "tough-cookie": "^4.0.0",
    "turndown": "^6.0.0"
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/node": "16",
    "@types/tough-cookie": "^4.0.2",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn run build && yarn run start",
    "ext": "ts"
  }
}
